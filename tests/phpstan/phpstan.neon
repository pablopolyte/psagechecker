parameters:
	bootstrap: tests/phpstan/bootstrap.php
	paths:
		- classes
		- controllers
		- psagechecker.php
	ignoreErrors:
		- '#Access to an undefined property ps_checkout::\$confirmUninstall\.#'
		- '#Access to an undefined property ps_checkout::\$bootstrap\.#'
	excludes_analyse:
		- vendor/

	level: 1
